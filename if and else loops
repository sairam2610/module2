        fact=fact*i;
    }
    printf("factorial=%d",fact);
    return 0;
}*/
/*#include<stdio.h>
int main() {
    int a,b,c;
    printf("Enter value of a: ");
    scanf("%d",&a);
    printf("Enter value of b: ");
    scanf("%d",&b);
    printf("Enter value of c: ");
    scanf("%d",&c);
    
    if(a>b&&a>c) {
        printf("%d is greater than the %d,%d",a,b,c);
    }
    else if(b>c&&b>a) {
        printf("%d is greater than the %d,%d",b,c,a);
    }
    else {
        printf("%d is greater than the %d,%d",c,a,b);
    }
    return 0;
}*/
