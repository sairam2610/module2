/*#include<stdio.h>
#include<math.h>
int main(){
    int a,b,c; 
    double discrimanant,root1,root2,realpart,imaginarypart;
    printf("enter value of a: ");
    scanf("%d",&a);
    printf("Enter value of b: ");
    scanf("%d",&b);
    printf("Enter value of c: ");
    scanf("%d",&c);
    
    discrimanant = b*b-4*a*c;
    
    if(discrimanant>0) {
        root1= (-b+ sqrt(discrimanant)) /(2*a);
        root2= (-b-sqrt(discrimanant)) /(2*a);
        printf("root1=%0.2lf root2=%0.2lf",root1,root2);
    }
    else if(discrimanant==0) {
        root1=root2=-b/(2*a);
        printf("root1=%0.2lf root2=%0.2lf",root1,root2);
    }
    else {
        realpart = -b/(2*a);
        imaginarypart = sqrt(discrimanant) / (2*a);
        printf("realpart=%0.2lf",realpart);
        printf("Imaginarypart=%0.2lf",imaginarypart);
    }
    return 0;
}*/
/*#include<stdio.h>
int main()
{
